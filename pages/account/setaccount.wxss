Page {
  background: #fff;
}

.container {
  width: 100%;
  display: flex;
  overflow: hidden;
}

.left_tabbar {
  width: 25%;
}

.right_content {
  width: 75%;
  padding-top:5px;
}

.left_tabbar_item {
  font-size: 26rpx;
  width: 100%;
  padding: 15px 0;
  text-align: center;
  border-bottom: 1rpx solid rgb(201,201,201);
  background:#f5f5f5;
  color:rgb(136,136,136);
}

.left_tabbar_list {
  background: #eee;
}

.left_tabbar .active {
  background: #fff;
  color:rgb(136,136,136);
  /* transition: all 0.5s; */
  /* animation: show 0.5s; */
}
.flexrowss {
display:flex;
display:-webkit-flex;
justify-content:space-start;
width:100%;
box-sizing:border-box;
align-items:top;

}


::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

@-webkit-keyframes show {
  0% {
    opacity: 0;
    transform: rotateY(180deg);
  }

  100% {
    opacity: 1;
    transform: rotateY(360deg);
  }
}

@keyframes show {
  0% {
    opacity: 0;
    transform: rotateY(180deg);
  }

  100% {
    opacity: 1;
    transform: rotateY(360deg);
  }
}



.tab_panel {
  margin: 0 20rpx;
  font-size: 26rpx;
}

.tab_panel view {
  padding:20px 6px;
}

