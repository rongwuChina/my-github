.tagbox{
  display: flex;
  flex-flow: row wrap;
  justify-content: space-start;
  height:auto;
  width:calc(100%-20);
  padding-left:10px;
  padding-right:10px;
  padding-top: 10px;
  position:fixed;
  top:45px;
  z-index:12;

}
.tagitem{
  padding:5px 10px;
  border:solid 1px #dcdcdc;
  display: block;
  margin-bottom: 10px;
  font-size: 12px;
  margin-right:2%;
  min-width:18%;
  box-sizing:border-box;

}

